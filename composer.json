{
    "name": "kanekescom/laravel-siasn-simpeg",
    "description": "Library to consume the SIASN SIMPEG BKN",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Achmad Hadi Kurnia",
            "email": "imachmadhadikurnia@gmail.com",
            "homepage": "https://achmadhadikurnia.com"
        }
    ],
    "homepage": "https://github.com/kanekescom/laravel-siasn-simpeg",
    "keywords": [
        "laravel",
        "siasn",
        "laravel-siasn-simpeg",
        "simpeg",
        "api",
        "bkn"
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0",
        "kanekescom/laravel-siasn-simpeg-api": "^1.0",
        "league/fractal": "^0.19.2",
        "league/csv": "^9.7",
        "prettus/l5-repository": "^2.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kanekescom\\Siasn\\Simpeg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kanekescom\\Siasn\\Simpeg\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanekescom\\Siasn\\Simpeg\\SimpegServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}